{
  "name": "onruf-central-control-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
  "seed": "ts-node-dev --transpile-only src/common/seed.ts",
    "extract:brd": "node ../tools/extract-brd.js '../OnRuf/BRDs/BRD - Malqaa_OnRuf_WebApp_CentralControlPanel.docx'",
  "parse:brd": "ts-node-dev --transpile-only src/scripts/parse-brd.ts",
  "migrate:deploy": "prisma migrate deploy",
  "test:int": "node --test dist/tests",
    "lint": "eslint . --ext .ts",
    "test": "node --test"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "argon2": "^0.41.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^5.17.0",
    "mammoth": "^1.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2"
  }
